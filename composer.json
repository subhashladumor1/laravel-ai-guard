{
    "name": "subhashladumor1/laravel-ai-guard",
    "description": "Laravel AI Guard - AI Cost & Budget Control for Laravel AI SDK",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Subhashladumor1\\LaravelAiGuard\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Subhash Ladumor"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "suggest": {
        "laravel/ai": "Use with Laravel AI SDK (12.x) for agents and recordFromResponse()"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Subhashladumor1\\LaravelAiGuard\\AiGuardServiceProvider"
            ],
            "aliases": {
                "AIGuard": "Subhashladumor1\\LaravelAiGuard\\Facades\\AIGuard"
            }
        }
    }
}
